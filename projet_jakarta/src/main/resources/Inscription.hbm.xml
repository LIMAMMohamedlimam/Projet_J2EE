<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cytech.projet_jakarta.model">
    <class name="com.cytech.projet_jakarta.model.Inscription" table="inscription" schema="GestionScolarite">


        <id name="id" column="idInscription">
            <generator class="identity"/>
        </id>

        <!-- Properties -->
        <property name="dateInscription" column="dateInscription" type="date" not-null="true"/>



    </class>
</hibernate-mapping>
